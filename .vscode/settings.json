  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeLinkDefinitions": "always"
  },

  "rust-analyzer.checkOnSave": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  }
}
